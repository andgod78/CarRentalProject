<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
<servlet>
    <servlet-name>AddCar</servlet-name>
    <servlet-class>pl.com.carrental.servlet.AddCar</servlet-class>
</servlet>
    <servlet-mapping>
        <servlet-name>AddCar</servlet-name>
        <url-pattern>/add</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>DeleteCar</servlet-name>
        <servlet-class>pl.com.carrental.servlet.DeleteCar</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DeleteCar</servlet-name>
        <url-pattern>/delete</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>RentCar</servlet-name>
        <servlet-class>pl.com.carrental.servlet.RentCar</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RentCar</servlet-name>
        <url-pattern>/rent</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>ReturnCar</servlet-name>
        <servlet-class>pl.com.carrental.servlet.ReturnCar</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ReturnCar</servlet-name>
        <url-pattern>/return</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>ReserveCar</servlet-name>
        <servlet-class>pl.com.carrental.servlet.ReserveCar</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ReserveCar</servlet-name>
        <url-pattern>/reserve</url-pattern>
    </servlet-mapping>
</web-app>